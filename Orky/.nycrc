{
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "build",
    "locale",
    "public"
  ],
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  },
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "all": true
}
